{
	"exclude": [
		"./tests",
		"./node_modules",
		"./dist"
	],
	"compilerOptions": {
		"target": "es2022",
		"module": "commonjs",
		"rootDir": "./src",
		"declaration": true,
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true /* Enables emit interoperability between CommonJS and ES Modules via creation of namespace objects for all imports. Implies 'allowSyntheticDefaultImports'. */,
	},
	"typedocOptions":{
		"entryPoints": [
			"./src/FunkoEjer/funko/funko.ts",
			"./src/FunkoEjer/funkoApp/funkoApp.ts",
			"./src/FunkoEjer/funkoCollection/funkoCollection.ts",
			"./src/FunkoEjer/funkoCommands/funkoCommands.ts",
		],
		"out": "./docs"
	}
}